<Window x:Class="FoolGame.Uil.Window.GameStart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GameStart" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Margin="50" Content="Новая игра" Click="Button_Click"/>
        <Button Grid.Row="1" Margin="50" Content="Загрузить" Click="Button_Click_1"/>
    </Grid>
</Window>
